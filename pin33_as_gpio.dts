/dts-v1/;
/plugin/;

/ {
    jetson-header-name = "Jetson 40pin Header";
    overlay-name = "GPIO pin bidirectional acess";
    compatible = "nvidia,p3768-0000+p3767-0000\0nvidia,p3768-0000+p3767-0001\0nvidia,p3768-0000+p3767-0003\0nvidia,p3768-0000+p3767-0004\0nvidia,p3768-0000+p3767-0005\0nvidia,p3768-0000+p3767-0000-super\0nvidia,p3768-0000+p3767-0001-super\0nvidia,p3768-0000+p3767-0003-super\0nvidia,p3768-0000+p3767-0004-super\0nvidia,p3768-0000+p3767-0005-super\0nvidia,p3509-0000+p3767-0000\0nvidia,p3509-0000+p3767-0001\0nvidia,p3509-0000+p3767-0003\0nvidia,p3509-0000+p3767-0004\0nvidia,p3509-0000+p3767-0005";


    fragment@0 {
        target = <&pinmux>;

        __overlay__ {
            pinctrl-names = "default";
            pinctrl-0 = <&jetson_io_pinmux>;
            
            jetson_io_pinmux: exp-header-pinmux {
		    hdr40-pin33 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
            hdr40-pin31 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
            hdr40-pin29 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
            hdr40-pin15 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
            hdr40-pin7 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
            hdr40-pin32 {nvidia,pins = "soc_gpio21_ph0";nvidia,tristate = <0x0>;nvidia,enable-input = <0x1>;nvidia,pull = <0x0>;};
	     };
        };
    };
};
